package com.jumploo.cms.bean;

public class Major {
	private String id;
	private String name;
	private FirstClassSubject firstClassSubject;

	public Major(String id, String name, FirstClassSubject firstClassSubject) {
		super();
		this.id = id;
		this.name = name;
		this.firstClassSubject = firstClassSubject;
	}

	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public FirstClassSubject getFirstClassSubject() {
		return firstClassSubject;
	}

	public void setFirstClassSubject(FirstClassSubject firstClassSubject) {
		this.firstClassSubject = firstClassSubject;
	}

}
